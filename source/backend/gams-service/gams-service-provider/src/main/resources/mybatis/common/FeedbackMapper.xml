<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fintech.gams.common.mapper.FeedbackMapper">
    <!-- 基础列 -->
    <sql id="feedbackColumns">
        a.ID as id,
        a.TYPE as type,
        a.LANG as lang,
        a.STATUS as status,
        a.NAME as name,
        a.EMAIL as email,
        a.MOBILE as mobile,
        a.ANSWER as answer,
        a.CONTENT as content,
        a.CREATE_BY as createBy,
        a.CREATE_DATE as createDate,
        a.UPDATE_BY as updateBy,
        a.UPDATE_DATE as updateDate
    </sql>

    <resultMap id="feedbackResultMap" type="com.fintech.gams.common.entity.Feedback">
        <id column="ID" property="id" />
        <result column="TYPE" property="type"/>
        <result column="LANG" property="lang"/>
        <result column="STATUS" property="status"/>
        <result column="NAME" property="name"/>
        <result column="EMAIL" property="email"/>
        <result column="MOBILE" property="mobile"/>
        <result column="ANSWER" property="answer"/>
        <result column="CONTENT" property="content"/>
        <result column="CREATE_BY" property="createBy"/>
        <result column="CREATE_DATE" property="createDate"/>
        <result column="UPDATE_BY" property="updateBy"/>
        <result column="UPDATE_DATE" property="updateDate"/>
    </resultMap>

    <!-- 普通插入 -->
    <insert id="insert" parameterType="com.fintech.gams.common.entity.Feedback">
        INSERT INTO FEEDBACK(
            ID ,
            TYPE ,
            LANG ,
            STATUS ,
            NAME ,
            EMAIL ,
            MOBILE ,
            ANSWER ,
            CONTENT ,
            CREATE_BY ,
            CREATE_DATE ,
            UPDATE_BY ,
            UPDATE_DATE 
        ) VALUES (
            #{id} ,
            #{type} ,
            #{lang} ,
            #{status} ,
            #{name} ,
            #{email} ,
            #{mobile} ,
            #{answer} ,
            #{content} ,
            #{createBy} ,
            #{createDate} ,
            #{updateBy} ,
            #{updateDate} 
        )
    </insert>
    <!-- 根据主键物理删除 -->
    <delete id="remove" parameterType="java.lang.Long">
        DELETE FROM FEEDBACK WHERE ID = #{id}
    </delete>

    <!-- 根据主键逻辑删除
    <update id="delete" parameterType="java.lang.Long">
        UPDATE FEEDBACK SET DEL_FLAG = 1 WHERE ID = #{id}
    </update>
    -->

    <!--根据条件是否插入数据 -->
    <insert id="insertSelective" parameterType="com.fintech.gams.common.entity.Feedback">
        INSERT INTO FEEDBACK
        <trim prefix="(" suffix=")" suffixOverrides=",">
        <if test="id != null">
                ID ,
        </if>
        <if test="type != null">
                TYPE ,
        </if>
        <if test="lang != null">
                LANG ,
        </if>
        <if test="status != null">
                STATUS ,
        </if>
        <if test="name != null">
                NAME ,
        </if>
        <if test="email != null">
                EMAIL ,
        </if>
        <if test="mobile != null">
                MOBILE ,
        </if>
        <if test="answer != null">
                ANSWER ,
        </if>
        <if test="content != null">
                CONTENT ,
        </if>
        <if test="createBy != null">
                CREATE_BY ,
        </if>
        <if test="createDate != null">
                CREATE_DATE ,
        </if>
        <if test="updateBy != null">
                UPDATE_BY ,
        </if>
        <if test="updateDate != null">
                UPDATE_DATE 
        </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
        <if test="id != null">
                #{id} ,
        </if>
        <if test="type != null">
                #{type} ,
        </if>
        <if test="lang != null">
                #{lang} ,
        </if>
        <if test="status != null">
                #{status} ,
        </if>
        <if test="name != null">
                #{name} ,
        </if>
        <if test="email != null">
                #{email} ,
        </if>
        <if test="mobile != null">
                #{mobile} ,
        </if>
        <if test="answer != null">
                #{answer} ,
        </if>
        <if test="content != null">
                #{content} ,
        </if>
        <if test="createBy != null">
                #{createBy} ,
        </if>
        <if test="createDate != null">
                #{createDate} ,
        </if>
        <if test="updateBy != null">
                #{updateBy} ,
        </if>
        <if test="updateDate != null">
                #{updateDate} 
        </if>
        </trim>
    </insert>

    <!-- 选择性更新数据 -->
    <update id="updateByPrimaryKeySelective" parameterType="com.fintech.gams.common.entity.Feedback">
        UPDATE FEEDBACK
           <set>
            <if test="type != null">
                TYPE = #{type} ,
            </if>
            <if test="lang != null">
                LANG = #{lang} ,
            </if>
            <if test="status != null">
                STATUS = #{status} ,
            </if>
            <if test="name != null">
                NAME = #{name} ,
            </if>
            <if test="email != null">
                EMAIL = #{email} ,
            </if>
            <if test="mobile != null">
                MOBILE = #{mobile} ,
            </if>
            <if test="answer != null">
                ANSWER = #{answer} ,
            </if>
            <if test="content != null">
                CONTENT = #{content} ,
            </if>
            <if test="createBy != null">
                CREATE_BY = #{createBy} ,
            </if>
            <if test="createDate != null">
                CREATE_DATE = #{createDate} ,
            </if>
            <if test="updateBy != null">
                UPDATE_BY = #{updateBy} ,
            </if>
            <if test="updateDate != null">
                UPDATE_DATE = #{updateDate} 
            </if>
           </set>
        WHERE ID = #{id}
    </update>

    <!-- 批量插入 -->
    <insert id="insertBatch" parameterType="list">
        INSERT INTO FEEDBACK(
            ID ,
            TYPE ,
            LANG ,
            STATUS ,
            NAME ,
            EMAIL ,
            MOBILE ,
            ANSWER ,
            CONTENT ,
            CREATE_BY ,
            CREATE_DATE ,
            UPDATE_BY ,
            UPDATE_DATE 
        )
        <foreach close=")" collection="list" item="item" index="index" open="(" separator="union">
            SELECT
            #{item.id} ,
            #{item.type} ,
            #{item.lang} ,
            #{item.status} ,
            #{item.name} ,
            #{item.email} ,
            #{item.mobile} ,
            #{item.answer} ,
            #{item.content} ,
            #{item.createBy} ,
            #{item.createDate} ,
            #{item.updateBy} ,
            #{item.updateDate} 
            FROM DUAL
        </foreach>
    </insert>

    <!-- 批量修改 -->
    <update id="updateBatch" parameterType="java.util.List">
        <foreach collection="list" item="item" index="index" open="begin" close=";end;" separator=";">
            UPDATE FEEDBACK SET
            TYPE = #{item.type} ,
            LANG = #{item.lang} ,
            STATUS = #{item.status} ,
            NAME = #{item.name} ,
            EMAIL = #{item.email} ,
            MOBILE = #{item.mobile} ,
            ANSWER = #{item.answer} ,
            CONTENT = #{item.content} ,
            CREATE_BY = #{item.createBy} ,
            CREATE_DATE = #{item.createDate} ,
            UPDATE_BY = #{item.updateBy} ,
            UPDATE_DATE = #{item.updateDate} 
            WHERE ID = #{item.id}
        </foreach>
    </update>

    <!-- 根据主键更新一条信息所有数据 -->
    <update id="updateByPrimaryKey" parameterType="com.fintech.gams.common.entity.Feedback">
        UPDATE FEEDBACK SET
            TYPE = #{type} ,
            LANG = #{lang} ,
            STATUS = #{status} ,
            NAME = #{name} ,
            EMAIL = #{email} ,
            MOBILE = #{mobile} ,
            ANSWER = #{answer} ,
            CONTENT = #{content} ,
            CREATE_BY = #{createBy} ,
            CREATE_DATE = #{createDate} ,
            UPDATE_BY = #{updateBy} ,
            UPDATE_DATE = #{updateDate} 
        WHERE ID = #{id}
    </update>

    <!-- 根据主键取数据 -->
    <select id="selectByPrimaryKey"  parameterType="java.lang.Long" resultType="com.fintech.gams.common.entity.Feedback">
        SELECT <include refid="feedbackColumns"/> FROM FEEDBACK a WHERE a.ID = #{id}
    </select>

    <!-- 可分页的对象查询 -->
    <select id="findList" resultType="com.fintech.gams.common.entity.Feedback">
        SELECT
        <include refid="feedbackColumns"/>
        FROM FEEDBACK a WHERE 1 = 1 <!-- and a.DEL_FLAG = 0-->
        <if test="type != null">
            AND a.TYPE LIKE '%'||#{type}||'%'
        </if>
        <if test="lang != null">
            AND a.LANG LIKE '%'||#{lang}||'%'
        </if>
        <if test="status != null">
            AND a.STATUS = #{status}
        </if>
        <if test="name != null">
            AND a.NAME LIKE '%'||#{name}||'%'
        </if>
        <if test="email != null">
            AND a.EMAIL LIKE '%'||#{email}||'%'
        </if>
        <if test="mobile != null">
            AND a.MOBILE LIKE '%'||#{mobile}||'%'
        </if>
        <if test="answer != null">
            AND a.ANSWER LIKE '%'||#{answer}||'%'
        </if>
        <if test="content != null">
            AND a.CONTENT LIKE '%'||#{content}||'%'
        </if>
        <if test="createBy != null">
            AND a.CREATE_BY = #{createBy}
        </if>
        <if test="createDate != null">
            AND a.CREATE_DATE = #{createDate}
        </if>
        <if test="updateBy != null">
            AND a.UPDATE_BY = #{updateBy}
        </if>
        <if test="updateDate != null">
            AND a.UPDATE_DATE = #{updateDate}
        </if>
    </select>

    <!-- 查询所有 -->
    <select id="selectAll" resultType="com.fintech.gams.common.entity.Feedback">
        SELECT
        <include refid="feedbackColumns"/>
        FROM FEEDBACK a <!--WHERE a.DEL_FLAG = 0-->
    </select>


    <!-- 可分页的对象查询 -->
    <select id="findInfoList" resultType="com.fintech.gams.common.entity.Feedback">
        SELECT
        <include refid="feedbackColumns"/>
        FROM FEEDBACK a WHERE 1 = 1 <!-- and a.DEL_FLAG = 0-->
        <if test="type != null">
            AND a.TYPE = #{type}
        </if>
        <if test="lang != null">
            AND a.LANG = #{lang}
        </if>
        <if test="status != null">
            AND a.STATUS = #{status}
        </if>
        <if test="content != null">
            AND a.CONTENT LIKE '%'||#{content}||'%'
        </if>
        <if test="startDate != null and endDate != null">
            AND a.CREATE_DATE &gt;= #{startDate}
            AND a.CREATE_DATE &lt;= #{endDate}
        </if>
    </select>


</mapper>

