<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fintech.gams.oauth2.mapper.OauthCodeMapper">
    <!-- 基础列 -->
    <sql id="oauthCodeColumns">
        a.ID as id,
        a.CODE as code,
        a.USER_NAME as userName,
        a.CLIENT_ID as clientId,
        a.CREATE_TIME as createTime
    </sql>

    <resultMap id="oauthCodeResultMap" type="com.fintech.gams.oauth2.entity.OauthCode">
        <id column="ID" property="id" />
        <result column="CODE" property="code"/>
        <result column="USER_NAME" property="userName"/>
        <result column="CLIENT_ID" property="clientId"/>
        <result column="CREATE_TIME" property="createTime"/>
    </resultMap>

    <!-- 普通插入 -->
    <insert id="insert" parameterType="com.fintech.gams.oauth2.entity.OauthCode">
        INSERT INTO OAUTH_CODE(
            ID ,
            CODE ,
            USER_NAME ,
            CLIENT_ID ,
            CREATE_TIME 
        ) VALUES (
            #{id} ,
            #{code} ,
            #{userName} ,
            #{clientId} ,
            #{createTime} 
        )
    </insert>
    <!-- 根据主键物理删除 -->
    <delete id="remove" parameterType="java.lang.Long">
        DELETE FROM OAUTH_CODE WHERE ID = #{id}
    </delete>

    <!-- 根据主键逻辑删除
    <update id="delete" parameterType="java.lang.Long">
        UPDATE OAUTH_CODE SET DEL_FLAG = 1 WHERE ID = #{id}
    </update>
    -->

    <!--根据条件是否插入数据 -->
    <insert id="insertSelective" parameterType="com.fintech.gams.oauth2.entity.OauthCode">
        INSERT INTO OAUTH_CODE
        <trim prefix="(" suffix=")" suffixOverrides=",">
        <if test="id != null">
                ID ,
        </if>
        <if test="code != null">
                CODE ,
        </if>
        <if test="userName != null">
                USER_NAME ,
        </if>
        <if test="clientId != null">
                CLIENT_ID ,
        </if>
        <if test="createTime != null">
                CREATE_TIME 
        </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
        <if test="id != null">
                #{id} ,
        </if>
        <if test="code != null">
                #{code} ,
        </if>
        <if test="userName != null">
                #{userName} ,
        </if>
        <if test="clientId != null">
                #{clientId} ,
        </if>
        <if test="createTime != null">
                #{createTime} 
        </if>
        </trim>
    </insert>

    <!-- 选择性更新数据 -->
    <update id="updateByPrimaryKeySelective" parameterType="com.fintech.gams.oauth2.entity.OauthCode">
        UPDATE OAUTH_CODE
           <set>
            <if test="code != null">
                CODE = #{code} ,
            </if>
            <if test="userName != null">
                USER_NAME = #{userName} ,
            </if>
            <if test="clientId != null">
                CLIENT_ID = #{clientId} ,
            </if>
            <if test="createTime != null">
                CREATE_TIME = #{createTime} 
            </if>
           </set>
        WHERE ID = #{id}
    </update>

    <!-- 批量插入 -->
    <insert id="insertBatch" parameterType="list">
        INSERT INTO OAUTH_CODE(
            ID ,
            CODE ,
            USER_NAME ,
            CLIENT_ID ,
            CREATE_TIME 
        )
        <foreach close=")" collection="list" item="item" index="index" open="(" separator="union">
            SELECT
            #{item.id} ,
            #{item.code} ,
            #{item.userName} ,
            #{item.clientId} ,
            #{item.createTime} 
            FROM DUAL
        </foreach>
    </insert>

    <!-- 批量修改 -->
    <update id="updateBatch" parameterType="java.util.List">
        <foreach collection="list" item="item" index="index" open="begin" close=";end;" separator=";">
            UPDATE OAUTH_CODE SET
            CODE = #{item.code} ,
            USER_NAME = #{item.userName} ,
            CLIENT_ID = #{item.clientId} ,
            CREATE_TIME = #{item.createTime} 
            WHERE ID = #{item.id}
        </foreach>
    </update>

    <!-- 根据主键更新一条信息所有数据 -->
    <update id="updateByPrimaryKey" parameterType="com.fintech.gams.oauth2.entity.OauthCode">
        UPDATE OAUTH_CODE SET
            CODE = #{code} ,
            USER_NAME = #{userName} ,
            CLIENT_ID = #{clientId} ,
            CREATE_TIME = #{createTime} 
        WHERE ID = #{id}
    </update>

    <!-- 根据主键取数据 -->
    <select id="selectByPrimaryKey"  parameterType="java.lang.Long" resultType="com.fintech.gams.oauth2.entity.OauthCode">
        SELECT <include refid="oauthCodeColumns"/> FROM OAUTH_CODE a WHERE a.ID = #{id}
    </select>

    <!-- 可分页的对象查询 -->
    <select id="findList" resultType="com.fintech.gams.oauth2.entity.OauthCode">
        SELECT
        <include refid="oauthCodeColumns"/>
        FROM OAUTH_CODE a WHERE 1 = 1 <!-- and a.DEL_FLAG = 0-->
        <if test="code != null">
            AND a.CODE = #{code}
        </if>
        <if test="userName != null">
            AND a.USER_NAME = #{userName}
        </if>
        <if test="clientId != null">
            AND a.CLIENT_ID = #{clientId}
        </if>
    </select>

    <!-- 查询所有 -->
    <select id="selectAll" resultType="com.fintech.gams.oauth2.entity.OauthCode">
        SELECT
        <include refid="oauthCodeColumns"/>
        FROM OAUTH_CODE a <!--WHERE a.DEL_FLAG = 0-->
    </select>

    <select id="findByCodeClientId" resultType="com.fintech.gams.oauth2.entity.OauthCode">
        SELECT
        <include refid="oauthCodeColumns"/>
        FROM OAUTH_CODE a
        WHERE a.CODE = #{code}
        AND a.CLIENT_ID = #{clientId}
    </select>

    <select id="findByUserNameClientId" resultType="com.fintech.gams.oauth2.entity.OauthCode">
        SELECT
        <include refid="oauthCodeColumns"/>
        FROM OAUTH_CODE a
        WHERE a.USER_NAME = #{userName}
        AND a.CLIENT_ID = #{clientId}
    </select>


</mapper>

