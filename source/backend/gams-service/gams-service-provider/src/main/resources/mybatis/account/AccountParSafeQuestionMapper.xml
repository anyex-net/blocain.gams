            </if>
            <if test="safeQuestion != null">
                SAFE_QUESTION = #{safeQuestion} ,
            </if>
            <if test="answer != null">
                ANSWER = #{answer} ,
            </if>
            <if test="createdBy != null">
                CREATED_BY = #{createdBy} ,
