        SELECT <include refid="accountParSafeQuestionColumns"/> FROM ACCOUNT_PAR_SAFE_QUESTION a WHERE a.ID = #{id}
    </select>

    <!-- 可分页的对象查询 -->
    <select id="findList" resultType="com.fintech.gams.account.entity.AccountParSafeQuestion">
        SELECT
        <include refid="accountParSafeQuestionColumns"/>
        FROM ACCOUNT_PAR_SAFE_QUESTION a WHERE 1 = 1 <!-- and a.DEL_FLAG = 0-->
        <if test="accountId != null">
            AND a.ACCOUNT_ID = #{accountId}
        </if>
        <if test="safeQuestion != null">
            AND a.SAFE_QUESTION LIKE '%'||#{safeQuestion}||'%'
        </if>
        <if test="answer != null">
            AND a.ANSWER LIKE '%'||#{answer}||'%'
        </if>
        <if test="createdBy != null">
            AND a.CREATED_BY = #{createdBy}
        </if>
        <if test="createdDate != null">
            AND a.CREATED_DATE = #{createdDate}
        </if>
        <if test="updatedBy != null">
